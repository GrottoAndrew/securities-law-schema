  "metadata": {
    "title": "Regulation D Bipartite Evidence-to-Control Satisfaction Graph",
    "version": "1.0.0",
    "lastModified": "2026-02-01T00:00:00Z",
    "catalogRef": "./regulation-d-controls.json",
    "disclaimer": "Sufficiency classifications require case-by-case legal review. This graph is illustrative, not legal advice.",
    "sufficiencyLegend": {
      "necessary": "Required — control cannot be satisfied without this evidence type",
      "sufficient": "Alone satisfies the control (rare — most controls need multiple evidence types)",
      "partial": "Contributes toward satisfying the control but is not alone sufficient",
      "supporting": "Corroborates other evidence but does not independently prove compliance",
      "conditional": "Only relevant when a specific offering condition applies"
    }
  },

  "evidenceTypes": [
    {
      "@id": "ev-type:subscription-agreement",
      "@type": "EvidenceType",
      "name": "Subscription Agreement",
      "description": "Executed agreement between issuer and investor containing representations, warranties, investment intent, suitability acknowledgments, and restrictive legends.",
      "category": "investor-documentation",
      "sourceType": "issuer-generated",
      "retentionYears": 7
    },
    {
      "@id": "ev-type:accredited-investor-questionnaire",
      "@type": "EvidenceType",
      "name": "Accredited Investor Questionnaire",
      "description": "Self-certification form completed by investor identifying which 501(a) category they qualify under and attesting to factual basis.",
      "category": "investor-documentation",
      "sourceType": "investor-generated",
      "retentionYears": 7
    },
    {
      "@id": "ev-type:irs-tax-returns",
      "@type": "EvidenceType",
      "name": "IRS Tax Returns (W-2, 1099, K-1, 1040)",
