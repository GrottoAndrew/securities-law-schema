      "sourceType": "government-sro-database",
      "retentionYears": 7,
      "freshnessDays": 30
    },
    {
      "@id": "ev-type:sec-edgar-enforcement-search",
      "@type": "EvidenceType",
      "name": "SEC EDGAR Enforcement Action Search",
